{
	"name": "vscode-pydata-viewer",
	"displayName": "vscode-pydata-viewer",
	"description": "Display PyData files (.npz .npy .pkl .pth) in VSCode.",
	"publisher": "Percy",
	"version": "0.0.14",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.70.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/haochengxia/vscode-pydata-viewer.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:pydata.preview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "pydata.preview",
				"displayName": "Python Data Preview",
				"selector": [
					{
						"filenamePattern": "*.npy"
					},
					{
						"filenamePattern": "*.npz"
					},
					{
						"filenamePattern": "*.pkl"
					},
					{
						"filenamePattern": "*.pck"
					},
					{
						"filenamePattern": "*.pickle"
					},
					{
						"filenamePattern": "*.pkl.gz"
					},
					{
						"filenamePattern": "*.pth"
					},
					{
						"filenamePattern": "*.ckpt"
					},
					{
						"filenamePattern": "*.pt"
					}
				]
			}
		],
		"configuration": [
			{
				"title": "vscode-pydata-viewer configuration",
				"properties": {
					"vscode-pydata-viewer.pythonPath": {
						"type": "string",
						"default": "default",
						"description": "The absolute path of python interpreter. `default` means no custom interpreter."
					},
					"vscode-pydata-viewer.scriptPath": {
						"type": "string",
						"default": "default",
						"description": "The absolute path of custom script. `default` means no custom script."
					}
				}
			}
		],
		"commands": [
			{
				"command": "vscode-pydata-viewer.toggleTruncation",
				"title": "Toggle Full Output",
				"icon": "$(expand-all)"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "vscode-pydata-viewer.toggleTruncation",
					"when": "activeCustomEditorId == 'pydata.preview'",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node --no-deprecation ./out/test/runTest.js"
	},
	"dependencies": {
		"python-shell": "^5.0.0"
	},
	"devDependencies": {
		"@types/mocha": "^9.1.1",
		"@types/node": "^20.0.0",
		"@types/vscode": "^1.70.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"@vscode/vsce": "^2.29.0",
		"eslint": "^8.16.0",
		"glob": "^10.3.10",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2"
	},
	"overrides": {
		"undici": "5.28.4"
	}
}

{
	"name": "vscode-pydata-viewer",
	"displayName": "vscode-pydata-viewer",
	"description": "Display PyData files (.npz .npy .pkl .pth) in VSCode.",
	"publisher": "Percy",
	"version": "0.0.8",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "^1.70.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/haochengxia/vscode-pydata-viewer.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:pydata.preview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "pydata.preview",
				"displayName": "Python Data Preview",
				"selector": [
					{
						"filenamePattern": "*.npy"
					},
					{
						"filenamePattern": "*.npz"
					},
					{
						"filenamePattern": "*.pkl"
					},
					{
						"filenamePattern": "*.pck"
					},
					{
						"filenamePattern": "*.pickle"
					},
					{
						"filenamePattern": "*.pth"
					},
					{
						"filenamePattern": "*.pt"
					}
				]
			}
		],
		"configuration": [
			{
				"title": "vscode-pydata-viewer configuration",
				"properties": {
					"vscode-pydata-viewer.pythonPath": {
						"type": "string",
						"default": "default",
						"description": "The absolute path of python interpreter. `default` means no custom interpreter."
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"python-shell": "^3.0.1"
	},
	"devDependencies": {
		"@types/vscode": "^1.70.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.2",
		"@vscode/test-electron": "^2.1.3",
		"python-shell": "^3.0.1"
	}
}
